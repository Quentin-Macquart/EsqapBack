{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "off",
    "import/no-unresolved": "off",
    "import/prefer-default-export": "off",
    "no-plusplus": "off",
    "no-shadow": "off",
    "no-param-reassign": 0,
    "arrow-body-style": "off",
    "eqeqeq": "off",
    "no-underscore-dangle": ["error"]
  }
}
